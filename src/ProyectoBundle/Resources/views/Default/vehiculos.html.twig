      <div class="content-top">
        <div class="about_wrapper">
          <h1>Vehiculos</h1>
        </div>
        <div class="text">
          {% for vehiculo in vehiculos %}
          <div class="grid_1_of_3 images_1_of_3">
            <div class="grid_1">
              <a href="/vehiculo/id={{vehiculo.id}}"><img src="{{vehiculo.imagen}}" title="continue reading" alt=""></a>
              <div class="grid_desc">
                <p class="title">{{vehiculo.nombre}}</p>
                <p class="descripcion_corta">{{vehiculo.descripcionCorta}}</p>
                <div class="price" style="height: 19px;">
                  <span class="reducedfrom">{{vehiculo.precio}}€</span>
                  <span class="actual"></span>
                </div>
                <div class="cart-button">
                  <div class="cart">
                    <button class="button">
                      <span>Details</span>
                    </button>
                  </div>
                  <button class="button">
                    <span>Cart</span>
                  </button>
                  <div class="clear"></div>
                </div>
              </div>
            </div>
            <div class="clear"></div>
          </div>
          {% endfor %}

          <div class="clear"></div>
        </div>
